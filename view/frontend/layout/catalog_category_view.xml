<page xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" layout="2columns-left" xsi:noNamespaceSchemaLocation="urn:magento:framework:View/Layout/etc/page_configuration.xsd">
    <body>
        <referenceContainer name="content">
            <block class="MageCloud\Faq\Block\Category\Faq"
                   name="magecloud.category.faq"
                   template="MageCloud_Faq::category/faq.phtml"
                   after="category.products">
                <arguments>
                    <argument name="viewModel" xsi:type="object">MageCloud\Faq\ViewModel\Category\Faq</argument>
                </arguments>
            </block>
        </referenceContainer>
    </body>
</page>
